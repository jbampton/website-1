<?xml version="1.0"?>
<svg:svg xmlns:xlink='http://www.w3.org/1999/xlink' zoomAndPan="magnify" preserveAspectRatio="xMidYMid" xmlns:svg='http://www.w3.org/2000/svg'>
  <svg:defs>
    <svg:style type="text/css"> @import url("risk.css");</svg:style>
  </svg:defs>
  <diagram xmlns="http://www.kite9.org/schema/adl" name="Pillars" style="kite9-layout: right; ">

    <table>
      <cell style=" kite9-occupies: 0 0; ">
        <box style="kite9-margin: 4pt;">
          <axis style="kite9-type: svg; kite9-margin: 6pt; ">
            <svg:text transform="rotate(-90)">Risk Score</svg:text>
          </axis>
        </box>
      </cell>
      <cell style=" kite9-occupies: 1 0; ">
        <box style="kite9-margin: 4pt; ">
          <matrix style="kite9-type: container; kite9-min-size: 200pt 200pt;">
            <symbol class="decal" theChar="1" ox="20" oy="120"/>
            <symbol class="decal" theChar="2" ox="180" oy="150"/>
            <symbol class="decal" theChar="3" ox="170" oy="10"/>
            <symbol class="decal" theChar="4" ox="120" oy="115"/>
            <symbol class="decal" theChar="5" ox="60" oy="70"/>
          </matrix>
        </box>
      
      </cell>
      
      <cell style=" kite9-occupies: 1 1; ">
        <box style="kite9-margin: 4pt; ">
          <axis style="kite9-type: svg; kite9-margin: 6pt; ">
            <svg:text>Payoff</svg:text>
          </axis>
        </box>
      </cell>
      
      <cell style="kite9-occupies: 2 2 0 1; kite9-padding: 10pt; kite9-layout: down; kite9-vertical-align: top; ">
        <keyitem style="kite9-horizontal-align: left;">
          <symbol theChar="1" ox="0" oy="0"/> 
          <keydesc>Dinner Party Is Boring</keydesc> 
        </keyitem>
        <keyitem style="kite9-horizontal-align: left;">
          <symbol theChar="2" ox="0" oy="0"/> 
          <keydesc>Not Enough Food
          (Buy extra snacks)</keydesc> 
        </keyitem>
        <keyitem style="kite9-horizontal-align: left;">
          <symbol theChar="3" ox="0" oy="0"/> 
          <keydesc>No Ingredients for Meal
          (Go shopping early)</keydesc> 
        </keyitem>
        <keyitem style="kite9-horizontal-align: left;">
          <symbol theChar="4" ox="0" oy="0"/> 
          <keydesc>Some Guests Can't Eat the Food
          (Check with guests first)</keydesc> 
        </keyitem>
        <keyitem style="kite9-horizontal-align: left;">
          <symbol theChar="5" ox="0" oy="0"/> 
          <keydesc>Ruin The Food (Practice)</keydesc> 
        </keyitem>
      </cell>
      
    </table>
    
    
    

    
    
  </diagram>
</svg:svg>